{
  "name": "bwt-team/laravel-request-logger",
  "description": "A Laravel 5 package to log HTTP requests",
  "keywords": ["laravel", "logs", "logger", "logging"],
  "license": "MIT",
  "authors": [
    {
      "name": "Chesanovskiy Denis",
      "email": "chesanovskiydv@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "moontoast/math": "^1.1.2",
    "wyrihaximus/html-compress": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "BwtTeam\\LaravelRequestLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BwtTeam\\LaravelRequestLogger\\Providers\\RequestLoggerServiceProvider"
      ],
      "aliases": {
        "RequestLogger": "BwtTeam\\LaravelRequestLogger\\Facade\\RequestLogger"
      }
    }
  }
}
